<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Document   : SpecialRules.hbm.xml
    Created on : 24. januar 2011, 10:50
    Author     : Glenn Rune StrandbrÃ¥ten
    Description:
        This file contains the hibernate mapping information between the SpecialRules
        table in the warhammerDB database and the myrmidia.Warhammer.SpecialRules class.
-->
<hibernate-mapping>
  <class mutable="false" name="myrmidia.Warhammer.SpecialRules" table="SpecialRules">
    <id name="id" type="integer" unsaved-value="0">
      <generator class="increment"/>
    </id>
    <property column="specialRule" length="35" name="rule"/>
  </class>
</hibernate-mapping>
