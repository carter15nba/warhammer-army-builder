<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.Warhammer.Warhammer.Case" table="CASES">
    <id name="ID" type="integer">
      <generator class="increment"/>
    </id>
    <many-to-one name="army" column="army_ID" cascade="all" unique="true" lazy="false" class="org.Warhammer.Warhammer.Army"/>
    <property column="OPPONENT_RACE" length="20" name="opponent">
      <type name="jcolibri.connector.databaseutils.EnumUserType">
        <param name="enumClassName">org.Warhammer.Warhammer.Case$Races</param>
      </type>
    </property>
    <property column="OUTCOME" length="10" name="outcome">
      <type name="jcolibri.connector.databaseutils.EnumUserType">
        <param name="enumClassName">org.Warhammer.Warhammer.Case$Outcomes</param>
      </type>
    </property>
  </class>
</hibernate-mapping>
