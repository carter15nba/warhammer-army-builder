<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="org.Warhammer.Warhammer.Equipment" table="EQUIPMENT">
    <id name="equipmentID" type="integer">
      <generator class="increment"/>
    </id>
    <property column="TYPE" name="equipmentType" length="20"/>
    <property column="COST" name="cost"/>
    <property column="NAME" name="name" length="30"/>
  </class>
</hibernate-mapping>
