<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="Warhammer.UtilityUnit" table="UTILITYUNIT">
    <id name="ID" type="integer">
      <generator class="increment"/>
    </id>
    <property column="NAME" name="name"/>
    <property column="COST" name="cost"/>
    <property column="MINUNITS" name="minNumber"/>
    <property column="ATTACK" name="attack"/>
    <property column="BALLISTICSKILL" name="ballisticSkill"/>
    <property column="INITIATIVE" name="initiative"/>
    <property column="LEADERSHIP" name="leadership"/>
    <property column="MOVEMENT" name="movement"/>
    <property column="STRENGTH" name="strength"/>
    <property column="TOUGHNESS" name="toughness"/>
    <property column="UNITTYPE" name="unitType">
      <type name="jcolibri.connector.databaseutils.EnumUserType">
        <param name="enumClassName">Warhammer.UtilityUnit$unitType</param>
      </type>
    </property>
    <property column="WEAPONSKILL" name="weaponSkill"/>
    <property column="WOUNDS" name="wounds"/>
  </class>
</hibernate-mapping>
